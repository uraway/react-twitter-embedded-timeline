{
  "name": "react-twitter-embedded-timeline",
  "version": "0.3.5",
  "description": "Simple React component for Twitter Embedded Timeline",
  "keywords": [
    "react",
    "react-component",
    "twitter",
    "timeline",
    "twitter-timeline",
    "embedded-timeline"
  ],
  "main": "lib/TwitterTimeline.js",
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "jsx"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel src --out-dir lib",
    "watch": "npm-watch",
    "serve": "node ./example/server.js",
    "example": "webpack --config ./example/webpack.config.js",
    "start": "npm run example && npm run serve"
  },
  "author": "Masato Urai",
  "license": "MIT",
  "homepage": "https://github.com/uraway/react-twitter-embedded-timeline",
  "repository": {
    "type": "git",
    "url": "git://github.com/uraway/react-twitter-embedded-timeline.git"
  },
  "bugs": {
    "url": "https://github.com/uraway/react-twitter-embedded-timeline/issues"
  },
  "peerDependencies": {
    "react": ">=0.14.7",
    "react-dom": ">=0.14.7"
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-eslint": "8.1.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "eslint": "4.14.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-loader": "^1.2.1",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.5.1",
    "npm-watch": "^0.1.1"
  }
}
